generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
          
model users {
  id Int @id @default(autoincrement())
  first_name String     @db.VarChar(255)
  last_name String      @db.VarChar(255)
  password String       @db.VarChar(255)
} 
